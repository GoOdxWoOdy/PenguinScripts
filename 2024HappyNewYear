        local Window = Library.CreateLib("每日任務腳本 - [UPDATE 4.66] King Legacy", "DarkTheme")
        
        
    
        
        
        local Acc = Window:NewTab("公告 or 說明")
        local AccSection = Acc:NewSection("此腳本並不具備自動打擊,拾取的功能")
        local AccSection = Acc:NewSection("此腳本是簡易製作 並不具備多項功能")
