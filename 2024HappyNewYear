end)


LOL:Button("浮板(Kickboard)", function()
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
    Sound.SoundId = "rbxassetid://9125530803"
    Sound:Play()
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
            Sound.SoundId = "rbxassetid://9125530803"
            Sound:Play()
            local CoreGui = game:GetService("StarterGui")

            game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "歡迎使用";
                Text = "By.企鵝";
                Icon = "rbxthumb://type=Asset&id=5082444247&w=150&h=150"
            })
            
            
            mouse = game.Players.LocalPlayer:GetMouse()
            tool = Instance.new("Tool")
            tool.RequiresHandle = false
            tool.Name = ("顯示浮板")
            tool.Activated:Connect(function()
            game.Players.LocalPlayer.Character.Humanoid.MaxHealth = math.huge
            game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
            do wait()
            local function GhostPlayer()
            local script = Instance.new('LocalScript', bypassed_fly)
            
            local LocalPlayer = game.Players.LocalPlayer
            local torso = LocalPlayer.Character:FindFirstChild("Torso")
            
            if torso == nil then
            torso = LocalPlayer.Character:FindFirstChild("LowerTorso")
            end
            
            local emitter = Instance.new("ParticleEmitter")
            emitter.Transparency = NumberSequence.new(0.5)
            emitter.Rate = 20
            emitter.Speed = NumberRange.new(3)
            emitter.Lifetime = NumberRange.new(1)
            emitter.SpreadAngle = Vector2.new(360, 360)
            emitter.Parent = torso
            emitter.Texture = ""
            emitter.Size = NumberSequence.new(1.5)
            emitter.Orientation = Enum.ParticleOrientation.VelocityPerpendicular
                
            getgenv().partfly = true
            while getgenv().partfly == true do
            local leg = LocalPlayer.Character:FindFirstChild("Right Leg")
            
            if leg == nil then
            leg = LocalPlayer.Character:FindFirstChild("RightLowerLeg")
            end
            
            local position = Vector3.new(leg.Position.X-1, leg.Position.Y-2, leg.Position.Z)
            local part = Instance.new("Part")
            part.Color = Color3.new(1,1,1)
            part.Material = Enum.Material.Plastic
            part.Transparency = 0
            part.Position = position
            part.Size = Vector3.new(10, 0.25, 10)
            part.Anchored = true
            part.Parent = workspace
            local decal = Instance.new("Decal")
            decal.Texture = ""
            decal.Face = "Top"
            decal.Transparency = 0.5
            decal.Parent = part
            wait()
            part:Destroy()
            end
            end
            coroutine.wrap(GhostPlayer)()
            end
            end)		
            tool.Parent = game.Players.LocalPlayer.Backpack
            wait()
            mouse = game.Players.LocalPlayer:GetMouse()
            tool = Instance.new("Tool")
            tool.RequiresHandle = false
            tool.Name = ("關閉浮板")
            tool.Activated:Connect(function()
            game.Players.LocalPlayer.Character.Humanoid.MaxHealth = math.huge
            game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
            do wait()
            getgenv().partfly = false
            end
            end)		
            tool.Parent = game.Players.LocalPlayer.Backpack
end)

LOL:Button("隱藏/現身(Hide/Reveal)", function()
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
    Sound.SoundId = "rbxassetid://9125530803"
    Sound:Play()
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
            Sound.SoundId = "rbxassetid://9125530803"
            Sound:Play()
            local CoreGui = game:GetService("StarterGui")

            game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "歡迎使用";
                Text = "By.企鵝";
                Icon = "rbxthumb://type=Asset&id=5082444247&w=150&h=150"
            })
            
            local part = Instance.new("Part")
            
            local position = Vector3.new(0,100000,0)
            part.Name = "Part"
            part.Color = Color3.new(0,0,0)
            part.Material = Enum.Material.Plastic
