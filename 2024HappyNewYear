        end
    end)

    PL:Toggle("自動開啟武裝色(Auto Armament Haki)", {Toggled = true, Description = "Haki"}, function(value)
        _G.AUTOHAKI = value
    end)
    
    spawn(function()
           game:GetService("RunService").RenderStepped:Connect(function()
            pcall(function()
                if _G.AUTOHAKI then
        if game.Players.LocalPlayer.Character.Haki.Value == 0 then
            game.Players.LocalPlayer.Character.Haki.Value = 1
            game:GetService("Players").LocalPlayer.Character.Services.Client.Armament:FireServer()
                     end
                end
                    end)
                    end)
        end)
    
        PL:Toggle("自動開啟見聞色(Auto Observation Haki)",  nil, function(a)
            _G.KenHaki = a
        end)
    
    task.spawn(function()
        while task.wait() do
            if _G.KenHaki then
            pcall(function()
            KenHaki()
            end)
        end
    end
    end)
    
    function KenHaki()
        local pl = game.Players.LocalPlayer.Character
      if pl.KenOpen.Value == false then
        game:service('VirtualInputManager'):SendKeyEvent(true, "Y", false, game)
        game:service('VirtualInputManager'):SendKeyEvent(true, "Y", false, game)
        end
    end
    
    
    PL:Toggle("視野清晰(No Fog)", "視野清晰No Fog", function(state)
        if state then
            getgenv().toggle = true
            while getgenv().toggle == true and wait() do
                game:GetService("Lighting").Brightness = 1
                game:GetService("Lighting").FogEnd = 100000
                game:GetService("Lighting").GlobalShadows = true
                game:GetService("Lighting").OutdoorAmbient = Color3.fromRGB(128, 128, 128)
            end
        else
            getgenv().toggle = false
            while getgenv().toggle == true and wait() do
            game:GetService("Lighting").Brightness = 1
            game:GetService("Lighting").FogEnd = 2500
            game:GetService("Lighting").GlobalShadows = true
            game:GetService("Lighting").OutdoorAmbient = Color3.fromRGB(128, 128, 128)
            end
        end
        end)
    
        PL:Toggle("King Legacy吸果(Bring Fruit)",_G.BringFruit,function(value)
            _G.BringFruit = value
        end)
        
        spawn(function()
            while wait() do
                if _G.BringFruit then
                    pcall(function()
                        for i,v in pairs(game.Workspace:GetChildren()) do
                            if v:IsA("Tool") then
                                v.Handle.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                            end
                        end	
                    end)
                end
            end
        end)
    
        PL:Seperator("模式(滑塊)")
    
        PL:Slider("幀率(FPS)",1,360,120, function(s)
            local fps = s
            if setfpscap then
                  setfpscap(fps)
            end
        end)
        PL:Slider("移動速度[預設16](Walk Speed[Preset 16])",1,1000,16,function(s)
            local walkspeedplayer = game:GetService("Players").LocalPlayer
            local walkspeedmouse = walkspeedplayer:GetMouse()
            
            local walkspeedenabled = false
            
            if walkspeedenabled == false then
            _G.WS = s;
            local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;
            Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
            Humanoid.WalkSpeed = _G.WS;
            end)
            Humanoid.WalkSpeed = _G.WS;
            
            walkspeedenabled = true
            end
        end)
        PL:Slider("跳躍高度[預設50](Jump Power[Preset 50])",1,1000,50, function(s)
            local jumppowerplayer = game:GetService("Players").LocalPlayer
                local jumppowermouse = jumppowerplayer:GetMouse()
                
                local jumppowerenabled = false
                
                if jumppowerenabled == false then
                _G.JP = s;
                local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;
                Humanoid:GetPropertyChangedSignal("JumpPower"):Connect(function()
                Humanoid.JumpPower = _G.JP;
                end)
                Humanoid.JumpPower = _G.JP;
                
                jumppowerenabled = true
                end
        end)
        PL:Slider("角色重力(Gravity)",1,196.2,196.2, function(s)
            game.Workspace.Gravity = s
            GravityOFF.Visible = true
            GravityON.Visible = true
        end)
    
    
    
    
