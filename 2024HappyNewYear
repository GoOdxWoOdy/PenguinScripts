                wait(0.2)
                game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = CFrame.new(-73.02116394042969, 200.036733627319336, 270.6345520019531)
                wait(0.2)
                game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = CFrame.new(-255.32254028320312, 200.036733627319336, 43.95767593383789)
                wait(0.2)
