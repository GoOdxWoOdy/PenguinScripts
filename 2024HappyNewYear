                wait(0.2)
                game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = CFrame.new(-9.38868522644043, 200.86018943786621, 20.220664978027344)
                wait(0.2)
                game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = CFrame.new(234.4788360595703, 200.036733627319336, 80.78605651855469)
                wait(0.2)
                game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = CFrame.new(-307.05517578125, 200.5426025390625, 218.19650268554688)
                wait(0.2)
                game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = CFrame.new(-33.06283187866211, 200.97708511352539, 12.600724220275879)
                wait(0.0000000001)
            end
        else
            AutoMonster = false
