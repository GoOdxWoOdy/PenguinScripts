    Sraid  =  Check:Label("")
    function i21oepu()
       if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Boss") then
        Sraid:Set("小隊怪物(Monster) : 目前已刷新(Spawned) [⭕]")   
               else
                Sraid:Set("小隊怪物(Monster) : 目前尚未刷新(Not Spawn) [❌]")    
               end
               end
   
        spawn(function()
       while task.wait() do
           pcall(function()
            i21oepu()
           end)
       end
        end)

