    TextButton5.FontSize = Enum.FontSize.Size14
    TextButton5.Text = "離開望遠鏡"
    TextButton5.TextScaled = true
    TextButton5.TextSize = 8
    TextButton5.TextWrapped = true
    
    TextButton5.MouseButton1Click:connect(function()
    game.Players.LocalPlayer.Character.Humanoid.RootPart.Anchored = false
    local player = game.Players.LocalPlayer
    player.CameraMaxZoomDistance = 1000
    player.CameraMinZoomDistance = -1
    workspace.CurrentCamera.FieldOfView = 70
    ScreenGui:Destroy()
    end)
    wait()
    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
    end
    end)		
    tool.Parent = game.Players.LocalPlayer.Backpack
end)

LOL:Button("長腳(longerrrleg)", function()
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
    Sound.SoundId = "rbxassetid://9125530803"
    Sound:Play()
            local Character = game.Players.LocalPlayer.Character

local Hum = {
"BodyTypeScale",
"BodyProportionScale",
"BodyWidthScale",
"BodyHeightScale",
"BodyDepthScale",
"HeadScale"
}

function Remove()
repeat task.wait() until Character.LeftFoot:FindFirstChild("OriginalSize")
Character.LeftFoot.OriginalSize:Destroy()
Character.LeftLowerLeg.OriginalSize:Destroy()
Character.LeftUpperLeg.OriginalSize:Destroy()
end

Character.LeftLowerLeg.LeftKneeRigAttachment.OriginalPosition:Destroy()
Character.LeftUpperLeg.LeftKneeRigAttachment.OriginalPosition:Destroy()
Character.LeftLowerLeg.LeftKneeRigAttachment:Destroy()
Character.LeftUpperLeg.LeftKneeRigAttachment:Destroy()

for i=1,6 do
Remove()
Character.Humanoid[Hum[i]]:Destroy()
end
end)

LOL:Button("F3X", function()
local Sound = Instance.new("Sound",game:GetService("SoundService"))
Sound.SoundId = "rbxassetid://9125530803"
Sound:Play()
loadstring(game:GetObjects("rbxassetid://4698064966")[1].Source)()
end)

LOL:Button("Disco Fog", function()
basics = {Color3.new(255/255,0/255,0/255),Color3.new(255/255,85/255,0/255),Color3.new(218/255,218/255,0/255),Color3.new(0/255,190/255,0/255),Color3.new(0/255,85/255,255/255),Color3.new(0/255,0/255,127/255),Color3.new(170/255,0/255,255/255),Color3.new(0/255,204/255,204/255),Color3.new(255/255,85/255,127/255),Color3.new(0/255,0/255,0/255),Color3.new(255/255,255/255,255/255)}
game.Lighting.FogStart = 25
game.Lighting.FogEnd = 300
while true do
wait(0.5)
game.Lighting.FogColor = basics[math.random(1,#basics)]
end
end)

LOL:Button("洞(Hole)", function()
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
    Sound.SoundId = "rbxassetid://9125530803"
    Sound:Play()
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
            Sound.SoundId = "rbxassetid://9125530803"
            Sound:Play()
            local CoreGui = game:GetService("StarterGui")

            CoreGui:SetCore("SendNotification", {
                Title = "Script Made By",
                Text = "GhostPlayer",
                Icon = "rbxassetid://29819383",
                Duration = 2.5,
            })
            
            if workspace:FindFirstChild("Hole") then
            workspace:FindFirstChild("Hole").CFrame = game.Players.LocalPlayer.Character.LeftFoot.CFrame
            else
            local Hole = Instance.new("Part")
            
            local position = Vector3.new(0,100000,0)
            Hole.Name = "Hole"
            Hole.Color = Color3.new(0,0,0)
            Hole.Material = Enum.Material.Neon
            Hole.Transparency = 0.1
            Hole.Position = position
            Hole.Size = Vector3.new(7,0,7)
            Hole.CastShadow = true
            Hole.Anchored = true
            Hole.CanCollide = true
            Hole.Parent = workspace
            
            Hole.Touched:connect(function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1,0)
            end)
            
            wait(0.5)
            workspace:FindFirstChild("Hole").CFrame = game.Players.LocalPlayer.Character.RightFoot.CFrame
            end
end)

