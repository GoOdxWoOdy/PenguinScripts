if game.PlaceId == 4520749081 then
    First_Sea = true
 elseif game.PlaceId == 6381829480 then
    Second_Sea = true
 elseif game.PlaceId == 5931540094 then
    Dungeon_Sea = true
 end

PL:Seperator("模式(切換)")


if Dungeon_Sea then

    PL:Toggle("副本安全區(測試)", "", function(state)
        if state then
            AutoMonster = true
            while AutoMonster == true and wait() do
                game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = CFrame.new(-46.8942985534668, 200.814945220947266, -98.98419952392578)
                wait(0.2)
                game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = CFrame.new(124.16812896728516, 200.81377410888672, -74.11831665039062)
                wait(0.2)
